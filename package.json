{
    "name": "DominionStats",
    "description": "My amazing LWC app",
    "version": "0.0.1",
    "author": "gik0geck0",
    "bugs": "https://github.com/Matt/DominionStats/issues",
    "dependencies": {
        "compression": "^1.7.4",
        "connect-pg-simple": "^7.0.0",
        "cookie-parser": "^1.4.6",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "fast-csv": "^4.3.6",
        "helmet": "^4.6.0",
        "node-postgres": "^0.6.2",
        "passport": "^0.5.0",
        "passport-google-oauth20": "^2.0.0",
        "pg": "^8.7.1",
        "postgres-migrations": "^5.3.0",
        "sequelize": "^6.6.2"
    },
    "devDependencies": {
        "@types/compression": "^1.7.1",
        "@types/cookie-parser": "^1.4.2",
        "@types/connect-pg-simple": "7.0.0",
        "@types/eslint": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/jest": "^26.0.24",
        "@types/passport": "^1.0.7",
        "@types/passport-google-oauth20": "^2.0.11",
        "@types/pg": "^8.6.1",
        "eslint": "^7.32.0",
        "eslint-plugin-inclusive-language": "^2.1.1",
        "husky": "^7.0.1",
        "lwc-services": "^3.1.2",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "prettier": "^2.3.2"
    },
    "engines": {
        "node": ">=10.13.0",
        "npm": ">=6.4.1",
        "yarn": ">=1.9.4"
    },
    "homepage": "https://github.com/gik0geck0/DominionStats",
    "keywords": [
        "lwc"
    ],
    "license": "MIT",
    "nodemonConfig": {
        "watch": [
            "src/server/**/*.ts"
        ],
        "ext": "ts",
        "ignore": [
            "src/**/*.spec.ts",
            "src/**/*.test.ts"
        ],
        "exec": "ts-node ./src/server/api.ts"
    },
    "repository": "https://github.com/gik0geck0/DominionStats",
    "scripts": {
        "build": "lwc-services build -m production -b rollup && tsc -b ./src/server",
        "build:development": "lwc-services build -b rollup",
        "clean": "rimraf -rf dist lib",
        "lint": "eslint ./src/**/*.ts",
        "postinstall": "husky install",
        "precommit": "npm run prettier:verify && [ $? -eq 0 ] && npm run lint",
        "prettier": "prettier --write \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
        "prettier:verify": "prettier --list-different \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
        "start": "node lib/server/api.js",
        "test:unit": "lwc-services test:unit",
        "test:unit:coverage": "lwc-services test:unit --coverage",
        "test:unit:debug": "lwc-services test:unit --debug",
        "test:unit:watch": "lwc-services test:unit --watch",
        "watch": "run-p watch:client watch:server",
        "watch:client": "lwc-services watch -b rollup",
        "watch:server": "nodemon"
    }
}
