exports[`lightning-base-combobox-item renders card 1`] = `
<lightning-base-combobox-item
  class="slds-listbox__option_entity"
>
  #shadow-root(open)
    <span
      class="slds-media__figure slds-listbox__option-icon"
    >
      <lightning-icon
        class="slds-icon-utility-search slds-icon_container"
      >
        #shadow-root(open)
          <lightning-primitive-icon>
            #shadow-root(open)
              <svg
                aria-hidden="true"
                class="slds-icon slds-icon-text-default slds-icon_small"
                data-key="search"
                focusable="false"
              >
                <use
                  xlink:href="/assets/icons/utility-sprite/svg/symbols.svg#search"
                />
              </svg>
          </lightning-primitive-icon>
      </lightning-icon>
    </span>
    <span
      class="slds-media__body"
    >
      <span
        class="slds-listbox__option-text slds-listbox__option-text_entity"
      >
        <span
          class="slds-truncate"
          title="Item Example"
        >
          Item Example
        </span>
      </span>
      <span
        class="slds-listbox__option-meta slds-listbox__option-meta_entity"
      >
        <span
          class="slds-truncate"
          title="Item example info"
        >
          Item example info
        </span>
      </span>
    </span>
</lightning-base-combobox-item>
`;

